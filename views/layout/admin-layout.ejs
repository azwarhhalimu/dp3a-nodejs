</head>

<body id="page-top">
  <div id="wrapper">
    <!-- Sidebar -->
    <%- include('partial-admin/sidebar.ejs') %>
      <!-- Sidebar -->
      <div id="content-wrapper" class="d-flex flex-column">
        <div data-turbo-preview id="content">
          <!-- TopBar -->
          <%- include("partial-admin/nav.ejs")%>
            <!-- Topbar -->
            <% if(typeof messages !=="undefined" ){ if (messages.success) { %>
              <div class="success">
                <%= messages.success %>
              </div>
              <% } %>
                <% if (messages.error) { %>
                  <div class="error">
                    <%= messages.error %>
                  </div>
                  <% }} %>

                    <!-- Container Fluid-->
                    <%-body%>
                      <!---Container Fluid-->
        </div>
        <!-- Footer -->
        <footer class="sticky-footer bg-white">
          <div class="container my-auto">
            <div class="copyright text-center my-auto">
              <span>copyright &copy;
                <script>
                  document.write(new Date().getFullYear());
                </script> - developed by
                <b><a href="https://indrijunanda.gitlab.io/" target="_blank">indrijunanda</a></b>
              </span>
            </div>
          </div>
        </footer>
        <!-- Footer -->
      </div>
  </div>

  <!-- Scroll to top -->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>


  <script src="/admin-template/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/admin-template/vendor/jquery-easing/jquery.easing.min.js"></script>
  <script src="/admin-template/js/ruang-admin.min.js"></script>



  <% if( typeof data_table!="undefined" ) {%>
    <link href="/admin-template/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
    <!-- Page level plugins -->
    <script src="/admin-template/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="/admin-template/vendor/datatables/dataTables.bootstrap4.min.js"></script>
    <script>
      $(document).ready(function () {
        $('#table').DataTable();
        $(".dc").percircle({
          progressBarColor: "#CC3366",

        });
      });
    </script>
    <%} %>


</body>

</html>